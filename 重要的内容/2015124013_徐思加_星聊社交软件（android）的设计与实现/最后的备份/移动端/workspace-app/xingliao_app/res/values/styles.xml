<resources>

    <style name="main_tab_bottom">
        <item name="android:gravity">center</item>
        <item name="android:paddingTop">15dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:button">@null</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    
    <style name="normalText" parent="@android:style/TextAppearance">
        <item name="android:textColor">#444</item>
        <item name="android:textSize">14sp</item>
    </style>
    
    
    <style name="some_used">
        <item name="android:layout_marginTop">10dip</item>
        <item name="android:layout_marginLeft">10dip</item>
        <item name="android:layout_marginRight">10dip</item>
        <item name="android:layout_marginBottom">10dip</item>
    </style>
    
    <style name="nearby_spinner_style">
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@null</item>
        <item name="android:layout_marginTop">6dip</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

</resources>
